<p id="notice"><%= notice %></p>

<h1>Listing Doctorprofiles</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Qualification</th>
      <th>Clinic</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doctorprofiles.each do |doctorprofile| %>
      <tr>
        <td><%= doctorprofile.firstname %></td>
        <td><%= doctorprofile.lastname %></td>
        <td><%= doctorprofile.qualification %></td>
        <td><%= doctorprofile.clinic %></td>
        <td><%= doctorprofile.user %></td>
        <td><%= link_to 'Show', doctorprofile %></td>
        <td><%= link_to 'Edit', edit_doctorprofile_path(doctorprofile) %></td>
        <td><%= link_to 'Destroy', doctorprofile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doctorprofile', new_doctorprofile_path %>
